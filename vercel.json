{
  "version": 2,
  "buildCommand": "npm install",
  "outputDirectory": ".",
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.atdevstudio.com"
        }
      ],
      "destination": "https://atdevstudio.com/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/api/contact",
      "destination": "/api/contact.js"
    },
    {
      "source": "/api/health",
      "destination": "/api/health.js"
    }
  ]
}
