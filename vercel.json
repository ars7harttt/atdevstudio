{
  "version": 2,
  "buildCommand": "npm install",
  "outputDirectory": ".",
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/api/contact",
      "destination": "/api/contact.js"
    },
    {
      "source": "/api/health",
      "destination": "/api/health.js"
    }
  ]
}
